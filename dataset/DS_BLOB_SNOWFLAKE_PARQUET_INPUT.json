{
	"name": "DS_BLOB_SNOWFLAKE_PARQUET_INPUT",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_BLOB_SNOWFLAKE_PARQUET_INPUT",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FolderType": {
				"type": "string",
				"defaultValue": "@pipeline().parameters.FolderType"
			},
			"EventTrigger": {
				"type": "string",
				"defaultValue": "@if(empty(@pipeline().parameters.EventTrigger),'','Fire Trigger')"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"folderPath": {
					"value": "@if(empty(dataset().EventTrigger),\nconcat(\n    dataset().FolderType,\n    '/',\n    formatDateTime(utcNow(),'yyyy'),'/',\n    formatDateTime(utcNow(),'MM'),'/',\n    formatDateTime(utcNow(),'dd')\n),\ndataset().FolderType)",
					"type": "Expression"
				},
				"container": "input"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}